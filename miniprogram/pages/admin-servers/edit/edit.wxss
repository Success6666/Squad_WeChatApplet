.page{padding:30rpx;background:#fff}
.form{background:#fff;padding:20rpx;border-radius:12rpx}
.field{margin-bottom:20rpx}
.label{font-size:26rpx;color:#666;margin-bottom:8rpx}
.input{width:100%;height:64rpx;border:1px solid #E5E7EB;border-radius:8rpx;padding:0 12rpx}
.picker{height:64rpx;line-height:64rpx;border:1px solid #E5E7EB;border-radius:8rpx;padding:0 12rpx}
.actions{display:flex;justify-content:flex-end}
.btn{background:#4096FF;color:#fff;border-radius:8rpx;padding:12rpx 24rpx}
.btn.outline{background:transparent;border:1px solid #D9D9D9;color:#666;margin-left:12rpx}

